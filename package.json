{
	"name": "starter-angular-eslint",
	"version": "1.0.0",
	"scripts": {
		"ng": "ng",
		"dev": "ng serve --open",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"lint": "ng lint --fix",
		"lint:css": "npx stylelint **/*.scss",
		"prepare": "husky install"
	},
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jeyofdev/starter-angular-eslint.git"
	},
	"keywords": [
		"javascript",
		"js",
		"angular",
		"eslint",
		"prettier",
		"typescript",
		"husky"
	],
	"author": "jeyofdev <jgregoire.pro@gmail.com>",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/jeyofdev/starter-angular-eslint.git/issues"
	},
	"dependencies": {
		"@angular/animations": "^15.2.0",
		"@angular/common": "^15.2.0",
		"@angular/compiler": "^15.2.0",
		"@angular/core": "^15.2.0",
		"@angular/forms": "^15.2.0",
		"@angular/platform-browser": "^15.2.0",
		"@angular/platform-browser-dynamic": "^15.2.0",
		"@angular/router": "^15.2.0",
		"rxjs": "~7.8.0",
		"stylelint-config-standard-scss": "^8.0.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.12.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^15.2.4",
		"@angular-eslint/builder": "15.2.1",
		"@angular-eslint/eslint-plugin": "15.2.1",
		"@angular-eslint/eslint-plugin-template": "15.2.1",
		"@angular-eslint/schematics": "15.2.1",
		"@angular-eslint/template-parser": "15.2.1",
		"@angular/cli": "~15.2.4",
		"@angular/compiler-cli": "^15.2.0",
		"@types/jasmine": "~4.3.0",
		"@typescript-eslint/eslint-plugin": "5.48.2",
		"@typescript-eslint/parser": "5.48.2",
		"eslint": "^8.33.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"jasmine-core": "~4.5.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.0.0",
		"lint-staged": "^13.2.1",
		"prettier": "^2.8.7",
		"stylelint": "^15.4.0",
		"stylelint-config-standard": "^32.0.0",
		"typescript": "~4.9.4"
	},
	"lint-staged": {
		"*.ts": [
			"prettier --write",
			"eslint --fix --max-warnings=0"
		],
		"*.scss": [
			"npx stylelint **/*.scss"
		],
		"*.js": "eslint --cache --fix --max-warnings=0"
	}
}
